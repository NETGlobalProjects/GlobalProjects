from movimientos_lineas_telefonos m left join telefonos t on m.imei = t.imei 
									left join lineas l on l.numero = m.numero 
									left join marcas mr on t.id_marca = mr.id_marca
									left join SO so on so.id_so = t.id_so 
									left join tipos_movimientos tm on tm.id_tipo_movimiento = m.id_tipo_movimiento 
									left join estados e on e.id_estado = m.id_estado
									 join propietarios p on p.id_propietario = m.id_propietario 
									 join t_empresas em on em.id_empresa = p.id_empresa 
									 join t_departamentos dp on dp.id_departamento = p.id_departamento 
								    join cargos c on c.id_cargo = p.id_cargo
									join motivos_movimientos mt on mt.id_motivo = m.id_motivo
									left join v_ModuloTelefonos_Equipos me on me.id_equipo = m.id_equipo

									
where m.no_movimiento=@no_movimiento</CommandText>
